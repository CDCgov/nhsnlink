{
  "ServiceInformation": {
    "Name": "Link Data Acquisition Worker Service",
    "ServiceName": "DataAcquisition.AcquisitionWorker",
    "Version": "0.1.1-dev",
    "GitCommit": "git_value"
  },
  "EnableSwagger": false,
  "KafkaConnection": {
    "BootstrapServers": [ "" ],
    "SaslProtocolEnabled": true,
    "SaslUsername": ""
  },
  "DatabaseProvider": "SqlServer",
  "ConnectionStrings": {
    "DatabaseConnection": "Server=localhost\\SQLEXPRESS; Initial Catalog=link-dev-botw-data;Integrated Security=true;Encrypt=false;TrustServerCertificate=True",
    "Redis": "localhost:6379,abortConnect=false,connectTimeout=5000,defaultDatabase=0,ssl=false,password=redis_password",
  },
  "Telemetry": {
    "EnableTelemetry": false,
    "EnableRuntimeInstrumentation": false,
    "EnableOtelCollector": false,
    "OtelCollectorEndpoint": "",
    "EnableAzureMonitor": false
  },
  "Serilog": {
    "Using": [ "Serilog.Sinks.Console", "Serilog.Sinks.Grafana.Loki" ],
    "MinimumLevel": {
      "Default": "Information"
    },
    "WriteTo": [
      {
        "Name": "GrafanaLoki",
        "Args": {
          "uri": "",
          "labels": [
            {
              "key": "app",
              "value": "link-cloud"
            },
            {
              "key": "component",
              "value": "DataAcquisition"
            }
          ],
          "propertiesAsLabels": [
            "app",
            "component"
          ]
        }
      },
      {
        "Name": "Console"
      }
    ]
  }
}