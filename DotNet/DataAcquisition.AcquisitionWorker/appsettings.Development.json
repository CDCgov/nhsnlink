{
  "ServiceInformation": {
    "ServiceName": "Link Data Acquisition Worker Service"
  },
  "EnableSwagger": false,
  "KafkaConnection": {
    "BootstrapServers": [ "localhost:9092/" ],
    "SaslProtocolEnabled": false
  },
  "DatabaseProvider": "SqlServer",
  "ConnectionStrings": {
    "DatabaseConnection": "Server=localhost\\SQLEXPRESS; Initial Catalog=link-dev-botw-data;Integrated Security=true;Encrypt=false;TrustServerCertificate=True",
    "Redis": "localhost:6379,abortConnect=false,connectTimeout=5000,defaultDatabase=0,ssl=false"
  },
  "DistributedLockSettings": {
    "Expiration": 10,
    "RetryDelay": 5,
    "MaxRetryCount": 3
  },
  "Telemetry": {
    "EnableTelemetry": false,
    "EnableRuntimeInstrumentation": false,
    "EnableOtelCollector": false,
    "OtelCollectorEndpoint": "",
    "EnableAzureMonitor": false
  },
  "Serilog": {
    "Using": [ "Serilog.Sinks.Console", "Serilog.Sinks.Grafana.Loki" ],
    "MinimumLevel": {
      "Default": "Information"
    },
    "WriteTo": [
      {
        "Name": "GrafanaLoki",
        "Args": {
          "uri": "",
          "labels": [
            {
              "key": "app",
              "value": "link-cloud"
            },
            {
              "key": "component",
              "value": "DataAcquisitionWorker"
            }
          ],
          "propertiesAsLabels": [
            "app",
            "component"
          ]
        }
      },
      {
        "Name": "Console"
      }
    ]
  }
}